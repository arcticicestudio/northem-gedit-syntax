<style-scheme id="northem-gedit-syntax" name="Northem" filename="northem-gedit-syntax.xml" version="1.0">
  <author>Arctic Ice Studio</author>
  <description>A north-bluish dark theme based on the same named color palette "Northem"</description>

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Color Palette                                                          [1] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Color Palette - Northem 1.0.1                                        [1.0] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <color name="darkest-gray"   value="#2D2D2E" />
  <color name="dark-gray"      value="#333334" />
  <color name="gray"           value="#383839" />
  <color name="light-gray"     value="#606062" />
  <color name="dirty-white"    value="#DEDEDE" />
  <color name="white"          value="#ECEEEF" />

  <color name="aqua"           value="#90CDC7" />
  <color name="light-blue"     value="#88B2CD" />
  <color name="blue"           value="#86C1D2" />
  <color name="dark-blue"      value="#5C81AE" />

  <color name="red"            value="#DF7574" />
  <color name="yellow"         value="#FCCE85" />
  <color name="orange"         value="#EFA070" />
  <color name="lime"           value="#C8CB79" />
  <color name="purple"         value="#C2A2C9" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [1.0]-+ -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [1]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Settings                                                               [2] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="text" foreground="white" />
  <style name="selection" foreground="white" background="dark-blue" />
  <style name="selection-unfocused" foreground="white" background="dark-blue" />
  <style name="current-line" foreground="white" background="gray" />
  <style name="line-numbers" foreground="light-gray" background="darkest-gray" />
  <style name="cursor" foreground="white" />
  <style name="secondary-cursor" use-style="cursor" />
  <style name="bracket-match" foreground="white" background="dark-blue" />
  <style name="bracket-mismatch" foreground="red" underline="true" />
  <style name="search-match" foreground="white" background="dark-blue" underline="true" />
  <style name="draw-spaces" foreground="light-gray" />
  <style name="right-margin" foreground="light-gray" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [2]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions                                                            [3] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions - Customs                                                [3.0] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="def:symbol" foreground="white" />
  <style name="def:other" foreground="white" />
  <style name="def:regexp" foreground="yellow" />
  <style name="def:verbatim" foreground="yellow" />
  <style name="def:escape" foreground="yellow" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3.0]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions - Defaults                                               [3.1] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions - Defaults - Variables                                 [3.1.0] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="def:string" foreground="dirty-white" />
  <style name="def:character" foreground="dirty-white" />
  <style name="def:special-char" foreground="yellow" />
  <style name="def:number" foreground="purple" />
  <style name="def:decimal" use-style="def:number" />
  <style name="def:base-n-integer" use-style="def:number" />
  <style name="def:floating-point" use-style="def:number" />
  <style name="def:complex" use-style="def:number" />
  <style name="def:variable" foreground="white" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3.1.0]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions - Defaults - Constants                                 [3.1.1] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="def:constant" foreground="aqua" bold="true" />
  <style name="def:namespace" foreground="dark-blue" />
  <style name="def:special-constant" foreground="dark-blue" />
  <style name="def:boolean" foreground="aqua" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3.1.1]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions - Defaults - Comments                                  [3.1.2] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="def:comment" foreground="light-gray" />
  <style name="def:note" use-style="def:comment" />
  <style name="def:doc-comment" use-style="def:comment" />
  <style name="def:doc-comment-element" foreground="dark-blue" />
  <style name="def:net-address-in-comment" foreground="light-gray" italic="true" underline="true" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3.1.2]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions - Defaults - Keywords                                  [3.1.3] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="def:keyword" foreground="blue" />
  <style name="def:class" foreground="aqua" />
  <style name="def:tag" use-style="def:keyword" />
  <style name="def:identifier" use-style="def:keyword" />
  <style name="def:attribute" use-style="def:class" />
  <style name="def:shebang" foreground="dark-blue" />
  <style name="def:preprocessor" foreground="dark-blue" />
  <style name="def:statement" use-style="def:keyword" />
  <style name="def:builtin" use-style="def:keyword" />
  <style name="def:reserved" use-style="def:keyword" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3.1.3]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Definitions - Defaults - Logicals                                  [3.1.4] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="def:function" foreground="lime" />
  <style name="def:operator" foreground="blue" />
  <style name="def:error" foreground="red" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3.1.4]-+ -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3.1]-+ -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [3]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Languages                                                              [4] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Languages - CSS                                                      [4.0] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="css:at-rules" use-style="def:keyword" />
  <style name="css:color" use-style="def:constant" />
  <style name="css:comment" use-style="def:comment" />
  <style name="css:decimal" use-style="def:decimal" />
  <style name="css:error" use-style="def:error" />
  <style name="css:keyword" use-style="def:keyword" />
  <style name="css:block" use-style="def:symbol" />
  <style name="css:string" use-style="def:string" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [4.0]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Languages - diff                                                     [4.1] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="diff:added-line" foreground="lime" />
  <style name="diff:removed-line" foreground="red" />
  <style name="diff:changed-line" foreground="yellow" />
  <style name="diff:special-case" use-style="def:string" />
  <style name="diff:location" use-style="def:keyword" />
  <style name="diff:diff-file" foreground="dark-blue" bold="true" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [4.1]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Languages - HTML                                                     [4.2] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="html:dtd" use-style="def:shebang" />
  <style name="html:tag" use-style="def:keyword" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [4.2]-+ -->

<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - +
     + Languages - Java                                                     [4.3] +
     +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - + -->
  <style name="java:escaped-character" use-style="def:escape" />
  <style name="java:string" use-style="def:string" />
  <style name="java:char" use-style="def:character" />
  <style name="java:external" use-style="def:preprocessor" />
  <style name="java:declaration" use-style="def:variable" />
  <style name="java:storage-class" use-style="def:keyword" />
  <style name="java:scope-declaration" use-style="def:keyword" />
  <style name="java:keyword" use-style="def:keyword" />
  <style name="java:null-value" use-style="def:keyword" />
  <style name="java:boolean" use-style="def:keyword" />
  <style name="java:number" use-style="def:number" />
  <style name="java:reserved" use-style="def:constant" />
  <style name="java:type" use-style="def:keyword" />
  <style name="java:method" use-style="def:function" />
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [4.3]-+ -->
<!-- +- - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - [4]-+ -->
</style-scheme>
